{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "include": ["src"],
        "exclude": ["src/index.js", "\"src/index.ts\""]
    },
    "plugins": ["jsdoc-plugin-typescript"],
    "typescript": {
        "moduleRoot": "src"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./api-docs/"
    }
}
